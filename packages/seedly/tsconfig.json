{
  "extends": "@kalink-ui/typescript-config/lib-react.json",
  "include": [
    "src",
    ".storybook/**/*",
    "eslint.config.mts",
    "vite.config.mts",
    "package.json"
  ],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "noEmit": false,
    "incremental": true,
    "composite": true,
    "types": ["react"],
    "declaration": true,
    "emitDeclarationOnly": true,
    "rootDir": ".",
    "outDir": "dist"
  }
}
