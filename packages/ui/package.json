{
  "name": "@kalink/ui",
  "version": "0.0.0",
  "sideEffects": false,
  "license": "MIT",
  "exports": {
    "./button": {
      "types": "./src/button.tsx",
      "import": "./dist/button.mjs",
      "require": "./dist/button.js"
    },
    "./card": {
      "types": "./src/card.tsx",
      "import": "./dist/card.mjs",
      "require": "./dist/card.js"
    },
    "./code": {
      "types": "./src/code.tsx",
      "import": "./dist/code.mjs",
      "require": "./dist/code.js"
    }
  },
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "build": "tsup",
    "dev": "tsup --watch",
    "tsc": "tsc --noEmit"
