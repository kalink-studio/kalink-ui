  "devDependencies": {
    "@kalink/eslint-config": "workspace:*",
    "@kalink/typescript-config": "workspace:*",
    "@turbo/gen": "^2.3.4",
    "@types/node": "^22.10.10",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tsup": "^8.3.6",
    "typescript": "5.7.3"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
