{
  "name": "@kalink-ui/dibbly",
  "description": "A set of tools for building UIs with React and TypeScript",
  "version": "0.5.2",
  "private": false,
  "license": "MIT",
  "author": "Louis Gollut",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kalink/kalink-ui.git"
  },
  "exports": {
