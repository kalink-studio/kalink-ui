{
  "name": "@kalink-ui/canopy",
  "description": "A set of tools for building custom PayloadCMS experiences",
  "version": "0.0.1",
  "private": false,
  "license": "MIT",
  "author": "Louis Gollut",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kalink/kalink-ui.git"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": "./dist/*"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "tsc -b --watch --preserveWatchOutput",
    "build": "tsc -b",
    "prepare": "pnpm run build",
    "lint": "eslint --max-warnings 0",
    "lint:fix": "pnpm lint --fix",
    "tsc": "tsc -b"
  },
  "devDependencies": {
    "@kalink-ui/eslint-config": "workspace:*",
    "@kalink-ui/typescript-config": "workspace:*",
    "@payloadcms/ui": "^3.58.0",
    "@types/node": "^24.6.2",
    "@types/react": "19.2.0",
    "@types/react-dom": "19.2.0",
    "eslint": "^9.37.0",
    "payload": "^3.58.0",
    "react": "^19.2.0",
    "typescript": "^5.9.3",
    "vite": "^7.1.9",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@payloadcms/ui": "^3.58.0",
    "payload": "^3.58.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "^5.8.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
