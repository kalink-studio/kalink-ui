{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "lint": {
      "dependsOn": []
    },
    "lint:fix": {
      "dependsOn": []
    },
    "tsc": {
      "dependsOn": ["^tsc"]
    },
    "generate:types": {
      "outputs": ["apps/**/payload-types.ts"],
      "dependsOn": []
    },
    "generate:importmap": {
      "outputs": ["apps/**/app/(payload)/admin/importMap.js"],
      "dependsOn": []
    },
    "payload:prepare": {
      "dependsOn": ["generate:importmap", "generate:types"]
    },
    "test": {
      "dependsOn": []
    },
    "test:watch": {
      "persistent": true
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
