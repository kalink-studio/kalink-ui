{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "lint": {
      "dependsOn": []
    },
    "lint:fix": {
      "dependsOn": []
    },
    "tsc": {
      "dependsOn": ["^tsc"]
    },
    "test": {
      "dependsOn": []
    },
    "test:watch": {
      "persistent": true
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
