{
  "name": "kalink-ui",
  "private": true,
  "pnpm": {
    "overrides": {
      "@types/react": "19.2.7",
      "@types/react-dom": "19.2.3"
    }
  },
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "dev:seedly": "turbo run dev --filter=@kalink-ui/seedly...",
    "lint": "turbo lint",
    "lint:fix": "turbo lint:fix",
    "tsc": "turbo tsc",
    "test": "turbo test",
    "test:coverage": "turbo test -- --coverage",
    "test:watch": "turbo test:watch",
    "format": "prettier \"**/*.{ts,tsx,md,json,yml}\" --list-different",
    "format:fix": "pnpm format --write",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "jiti": "^2.6.1",
    "prettier": "^3.7.4",
    "turbo": "^2.7.1",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.26.1",
  "engines": {
    "node": ">=24"
  }
}
